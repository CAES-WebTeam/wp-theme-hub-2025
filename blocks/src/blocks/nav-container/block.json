{
  "apiVersion": 3,
  "name": "caes-hub/field-report-navigation",
  "title": "Field Report Navigation",
  "category": "design",
  "description": "Navigation menu with flyout support and mobile hamburger menu",
  "keywords": [
    "navigation",
    "menu",
    "nav"
  ],
  "textdomain": "caes-hub",
  "attributes": {
    "blockId": {
      "type": "string",
      "default": ""
    },
    "hoverDelay": {
      "type": "number",
      "default": 300
    },
    "mobileBreakpoint": {
      "type": "string",
      "default": "768px"
    },
    "mobileMenuLabel": {
      "type": "string",
      "default": "Menu"
    }
  },
  "providesContext": {
    "fieldReport/navigationId": "blockId",
    "fieldReport/hoverDelay": "hoverDelay"
  },
  "supports": {
    "html": false,
    "reusable": true,
    "spacing": {
      "padding": true,
      "margin": true
    },
    "color": {
      "background": true,
      "text": true
    }
  },
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "viewScript": "file:./view.js",
  "render": "file:./render.php"
}