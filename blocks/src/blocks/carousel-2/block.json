{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "caes-hub/carousel-2",
	"version": "0.1.0",
	"title": "New Carousel",
	"category": "theme",
	"icon": "slides",
	"description": "Displays a carousel of hand selected content, or a filtered list. Made for the CAES Field Report theme.",
	"example": {},
	"supports": {
		"html": false,
		"interactivity": true
	},
	"attributes": {
		"handSelectPosts": {
			"type": "boolean",
			"default": false
		},
		"selectedPosts": {
			"type": "array",
			"default": []
		},
		"categories": {
			"type": "array",
			"default": []
		},
		"postType": {
			"type": "string",
			"default": "post"
		},
		"orderBy": {
			"type": "string",
			"default": "desc"
		},
		"numberOfPosts": {
			"type": "number",
			"default": 4
		}
	},
	"textdomain": "carousel-2",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css",
	"render": "file:./render.php",
    "viewScriptModule": "file:./view.js"
}
