{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "caes-hub/pub-details-number",
  "version": "0.1.0",
  "title": "Publication Number",
  "category": "theme",
  "icon": "text-page",
  "description": "Displays the publication number.",
  "supports": {
    "align": true,
    "color": {
      "background": true,
      "text": true
    },
    "spacing": {
      "padding": true,
      "margin": true
    },
    "typography": {
      "fontSize": true,
      "lineHeight": true,
      "textAlign": true
    }
  },
  "attributes": {
    "link": {
      "type": "string",
      "default": ""
    },
    "showTooltip": {
      "type": "boolean",
      "default": false
    },
    "displayAsInfo": {
      "type": "boolean",
      "default": false
    }
  },
  "textdomain": "pub-details-number",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "render": "file:./render.php",
  "viewScript": "file:./view.js"
}