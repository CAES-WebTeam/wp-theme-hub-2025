{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "caes-hub/pub-series-nav",
  "version": "0.1.0",
  "title": "Publication Series Nav",
  "category": "theme",
  "icon": "text-page",
  "description": "Displays previous/next navigation for a publication series.",
  "supports": {
    "color": {
      "background": true,
      "text": true
    },
    "spacing": {
      "padding": true,
      "margin": true
    },
    "typography": {
      "fontSize": true,
      "lineHeight": true,
      "fontFamily": true,
      "fontWeight": true,
      "fontStyle": true,
      "textDecoration": true,
      "textTransform": true,
      "letterSpacing": true
    }
  },
  "attributes": {
    "direction": {
      "type": "string",
      "default": "previous",
      "enum": [
        "previous",
        "next"
      ]
    },
    "showPublicationNumber": {
      "type": "boolean",
      "default": true
    }
  },
  "textdomain": "pub-series-nav",
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css",
  "render": "file:./render.php"
}