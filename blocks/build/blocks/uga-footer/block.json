  "name": "caes-hub/uga-footer",
  "version": "0.1.0",
  "title": "UGA Footer",
  "category": "widgets",
  "icon": "block-default",
  "description": "UGA footer option for the UGA CAES Field Report 2025 theme.",
  "supports": {
    "html": false
