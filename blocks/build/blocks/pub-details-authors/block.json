{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "caes-hub/pub-details-authors",
  "version": "0.1.0",
  "title": "Publication Authors",
  "category": "theme",
  "icon": "text-page",
  "description": "Displays a publication's authors.",
  "acf": {
    "mode": "preview",
    "renderTemplate": "./render.php"
  },
  "supports": {
    "anchor": true,
    "color": {
      "background": true,
      "text": true
    },
    "spacing": {
      "padding": true,
      "margin": true
    },
    "typography": {
      "fontSize": true,
      "lineHeight": true,
      "textAlign": true
    }
  },
  "attributes": {
    "showHeading": {
      "type": "boolean",
      "default": true
    },
    "customHeading": {
      "type": "string",
      "default": ""
    },
    "type": {
      "type": "string",
      "default": "authors"
    },
    "authorsAsSnippet": {
      "type": "boolean",
      "default": false
    }
  },
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css"
}