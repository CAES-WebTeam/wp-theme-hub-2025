{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 3,
  "name": "caes-hub/pub-details-status",
  "version": "0.1.0",
  "title": "Publication Status",
  "category": "theme",
  "icon": "text-page",
  "description": "Displays a status badge if publication has been recently published, renewed, or revised.",
  "acf": {
    "mode": "preview",
    "renderTemplate": "./render.php"
  },
  "supports": {
    "anchor": true,
    "color": {
      "background": true,
      "text": true
    },
    "spacing": {
      "padding": true,
      "margin": true
    }
  },
  "attributes": {
    "showAsBadge": {
      "type": "boolean",
      "default": true
    }
  },
  "editorScript": "file:./index.js",
  "editorStyle": "file:./index.css",
  "style": "file:./style-index.css"
}